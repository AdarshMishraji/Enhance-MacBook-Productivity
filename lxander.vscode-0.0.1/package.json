        },
        "lxander.vscode.touchBar.gitpush": {
          "type": "boolean",
          "default": true,
          "description": "Enable git push"
        },
        "lxander.vscode.touchBar.gitpull": {
          "type": "boolean",
          "default": true,
          "description": "Enable git pull"
        },
        "lxander.vscode.touchBar.checkoutTo": {
          "type": "boolean",
          "default": true,
          "description": "Enable git checkoutTo"
