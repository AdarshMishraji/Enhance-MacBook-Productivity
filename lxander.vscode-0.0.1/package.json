{
  "name": "lxander-vscode",
  "displayName": "Lxander VS Code",
  "description": "VS Code useful extensions",
  "version": "0.0.1",
  "publisher": "lxander",
  "engines": {
    "vscode": "^1.74.3"
  },
  "categories": [
    "Themes",
    "Keymaps"
  ],
  "galleryBanner": {
    "color": "#000000",
    "theme": "dark"
  },
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "tags": [
    "js",
    "jsx",
    "osx",
    "git",
    "css",
    "dark",
    "blue",
    "icon",
    "scss",
    "html",
    "clean",
    "green",
    "macOS",
    "vivid",
    "theme",
    "yellow",
    "windows",
    "orginal",
    "balanced",
    "saturated",
    "beautiful",
    "dark theme",
    "color-theme",
    "web development"
  ],
  "contributes": {
    "commands": [
      {
        "command": "editor.action.goToReferences",
        "title": "‚Üí∆í Ref"
      },
      {
        "command": "workbench.action.togglePanel",
        "title": "‚¨ì Term"
      },
      {
        "command": "editor.action.rename",
        "title": "‚úé Rename"
      },
      {
        "command": "workbench.action.closeOtherEditors",
        "title": "‚ùå Close"
      },
      {
        "command": "workbench.action.toggleZenMode",
        "title": "‚ñ° Zen"
      },
      {
        "command": "workbench.files.action.showActiveFileInExplorer",
        "title": "‚äô Target"
      },
      {
        "command": "editor.action.organizeImports",
        "title": "üëá Imports"
      },
      {
        "command": "editor.foldAll",
        "title": "‚è´ Fold"
      },
      {
        "command": "editor.unfoldAll",
        "title": "‚è¨ UnFold"
      },
      {
        "command": "workbench.action.toggleSidebarVisibility",
        "title": "Side Bar"
      },
      {
        "command": "workbench.action.terminal.toggleTerminal",
        "title": "Terminal"
      },
      {
        "command": "lxander.vscode.touchBar.enableFocusMode",
        "title": "üßò‚Äç‚ôÄÔ∏è Focus"
      },
      {
        "command": "editor.action.copyLinesDownAction",
        "title": "‚ò∞‚Üì Copy"
      },
      {
        "command": "workbench.files.action.focusFilesExplorer",
        "title": "1Files",
        "icon": "./images/files-activity-bar.png"
      },
      {
        "command": "workbench.view.search",
        "title": "2Search",
        "icon": "./images/search-activity-bar.png"
      },
      {
        "command": "workbench.view.scm",
        "title": "3Source",
        "icon": "./images/scm-activity-bar.png"
      },
      {
        "command": "workbench.view.debug",
        "title": "4Debug",
        "icon": "./images/debug-activity-bar.png"
      },
      {
        "command": "workbench.view.extensions",
        "title": "5Extensions",
        "icon": "./images/extensions-activity-bar.png"
      },
      {
        "command": "lxander.vscode.touchBar.night",
        "title": "Night"
      },
      {
        "command": "lxander.vscode.touchBar.day",
        "title": "Day"
      },
      {
        "command": "lxander.vscode.touchBar.syncBranch",
        "title": "Git:Sync"
      },
      {
        "command": "lxander.vscode.touchBar.gitcommit",
        "title": "Git:Commit"
      },
      {
        "command": "lxander.vscode.touchBar.gitpush",
        "title": "Git:Push"
      },
      {
        "command": "lxander.vscode.touchBar.gitpull",
        "title": "Git:Pull"
      },
      {
        "command": "lxander.vscode.touchBar.checkoutTo",
        "title": "Git:Checkout"
      },
      {
        "command": "lxander.vscode.touchBar.stageAll",
        "title": "Git:Stage"
      }
    ],
    "menus": {
      "touchBar": [
        {
          "command": "editor.action.goToReferences",
          "group": "group_1",
          "when": "config.lxander.vscode.touchBar.goToReferences"
        },
        {
          "command": "workbench.action.togglePanel",
          "group": "group_2",
          "when": "config.lxander.vscode.touchBar.terminal"
        },
        {
          "command": "workbench.action.toggleSidebarVisibility",
          "group": "group_1",
          "when": "config.lxander.vscode.touchBar.focusCode"
        },
        {
          "command": "workbench.action.terminal.toggleTerminal",
          "group": "group_1",
          "when": "config.lxander.vscode.touchBar.focusCode"
        },
        {
          "command": "lxander.vscode.touchBar.enableFocusMode",
          "group": "group_1",
          "when": "config.lxander.vscode.touchBar.focusMode"
        },
        {
          "command": "editor.action.rename",
          "group": "group_1",
          "when": "config.lxander.vscode.touchBar.rename"
        },
        {
          "command": "workbench.action.closeOtherEditors",
          "group": "group_2",
          "when": "config.lxander.vscode.touchBar.closeOtherEditors"
        },
        {
          "command": "workbench.action.toggleZenMode",
          "group": "group_2",
          "when": "config.lxander.vscode.touchBar.toggleZenMode"
        },
        {
          "command": "workbench.files.action.showActiveFileInExplorer",
          "group": "group_2",
          "when": "config.lxander.vscode.touchBar.showActiveFileInExplorer"
        },
        {
          "command": "editor.action.organizeImports",
          "group": "group_1",
          "when": "config.lxander.vscode.touchBar.organizeImports"
        },
        {
          "command": "editor.foldAll",
          "group": "group_1",
          "when": "config.lxander.vscode.touchBar.foldAll"
        },
        {
          "command": "editor.unfoldAll",
          "group": "group_1",
          "when": "config.lxander.vscode.touchBar.unfoldAll"
        },
        {
          "command": "editor.action.copyLinesDownAction",
          "group": "group_1",
          "when": "config.lxander.vscode.touchBar.copyLinesDownAction"
        },
        {
          "command": "lxander.vscode.touchBar.syncBranch",
          "when": "config.lxander.vscode.touchBar.syncBranch",
          "group": "activityBar"
        },
        {
          "command": "lxander.vscode.touchBar.gitcommit",
          "when": "config.lxander.vscode.touchBar.gitcommit",
          "group": "activityBar"
        },
        {
          "command": "lxander.vscode.touchBar.gitpush",
          "when": "config.lxander.vscode.touchBar.gitpush",
          "group": "activityBar"
        },
        {
          "command": "lxander.vscode.touchBar.gitpull",
          "when": "config.lxander.vscode.touchBar.gitpull",
          "group": "activityBar"
        },
        {
          "command": "lxander.vscode.touchBar.checkoutTo",
          "when": "config.lxander.vscode.touchBar.checkoutTo",
          "group": "activityBar"
        },
        {
          "command": "lxander.vscode.touchBar.stageAll",
          "when": "config.lxander.vscode.touchBar.stageAll",
          "group": "activityBar"
        },
        {
          "command": "workbench.files.action.focusFilesExplorer",
          "when": "config.lxander.vscode.touchBar.showActivityBar && config.lxander.vscode.touchBar.showExplorer",
          "group": "activityBar"
        },
        {
          "command": "workbench.view.search",
          "when": "config.lxander.vscode.touchBar.showActivityBar && config.lxander.vscode.touchBar.search",
          "group": "activityBar"
        },
        {
          "command": "workbench.view.scm",
          "when": "config.lxander.vscode.touchBar.showActivityBar && config.lxander.vscode.touchBar.scm",
          "group": "activityBar"
        },
        {
          "command": "workbench.view.debug",
          "when": "config.lxander.vscode.touchBar.showActivityBar && config.lxander.vscode.touchBar.debug",
          "group": "activityBar"
        },
        {
          "command": "workbench.view.extensions",
          "when": "config.lxander.vscode.touchBar.showActivityBar && config.lxander.vscode.touchBar.extensions",
          "group": "activityBar"
        },
        {
          "command": "lxander.vscode.touchBar.night",
          "when": "config.lxander.vscode.touchBar.themeToggle",
          "group": "themeToggle"
        },
        {
          "command": "lxander.vscode.touchBar.day",
          "when": "config.lxander.vscode.touchBar.themeToggle",
          "group": "themeToggle"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "TouchBar Pro",
      "properties": {
        "lxander.vscode.touchBar.goToReferences": {
